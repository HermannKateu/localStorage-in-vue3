{
  "name": "holiday-project",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "test:e2e": "npx cypress open",
    "build": "vue-tsc --noEmit && vite build",
    "test:unit": "jest",
    "preview": "vite preview",
    "openapi-generate-authentication": "openapi --useOptions --client axios --input ./specs/authentication.yaml --output ./src/services/authentication",
    "openapi-generate-user": "openapi --useOptions --client axios --input specs/user.yaml --output ./src/services/user",
    "openapi-generate-holiday": "openapi --useOptions --client axios --input ./specs/holiday.yaml --output ./src/services/holiday",
    "openapi-generate": "npm run openapi-generate-authentication && npm run openapi-generate-holiday && npm run openapi-generate-user"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.17.12",
    "@vuelidate/core": "^2.0.0",
    "@vuelidate/validators": "^2.0.0",
    "axios": "^1.3.4",
    "cypress": "^9.7.0",
    "dayjs": "^1.11.7",
    "pinia": "^2.0.33",
    "vue": "^3.2.25",
    "vue-i18n": "~9.1.9",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@types/jest": "^28.1.1",
    "@types/mocha": "^9.1.1",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue/vue3-jest": "^28.0.0",
    "autoprefixer": "^10.4.7",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^28.1.1",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.9.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "openapi-typescript-codegen": "^0.23.0",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.0.24",
    "ts-jest": "^28.0.4",
    "typescript": "^4.9.5",
    "vite": "^2.9.9",
    "vue-tsc": "^0.34.7"
  }
}
